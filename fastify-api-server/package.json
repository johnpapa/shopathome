{
  "name": "fastify-api-server",
  "version": "1.0.0",
  "description": "Fastify API server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "svelte-dev": "VITE_FASTIFY=true VITE_API=http://0.0.0.0:3000/api npm --prefix ../svelte-app run dev",
    "svelte-prod": "VITE_FASTIFY=true VITE_API=http://0.0.0.0:3000/api npm --prefix ../svelte-app run build && npm --prefix ../svelte-app start",
    "start-svelte-dev": "concurrently \"npm start\" \"npm run svelte-dev\"",
    "start-svelte-prod": "concurrently \"npm start\" \" npm run svelte-prod \""
  },
  "keywords": [
    "fastify",
    "api",
    "server"
  ],
  "engines": {
    "node": "20"
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "fastify": "^4.28.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
