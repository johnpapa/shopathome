{
  "routes": [
    {
      "route": "/.auth/login/facebook",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/google",
      "statusCode": "404"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/api/products",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/x/products",
      "allowedRoles": ["preferred"]
    },
    {
      "route": "/api/discounts",
      "allowedRoles": ["preferred"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/404.html"
    }
  ]
}
