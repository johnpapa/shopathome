{
  "name": "svelte-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "startx": "sirv public --cors --single --no-clear --port 5001",
    "validate": "svelte-check",
    "dev": "vite --port 5001",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "local-public": "npx @azure/static-web-apps-cli@latest start ./public --api-location http://localhost:7071",
    "local-x": "npx @azure/static-web-apps-cli@latest start ./public --api-location ../api",
    "local": "npx @azure/static-web-apps-cli@latest start http://localhost:5001 --api-location ../api "
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.5.2",
    "@tsconfig/svelte": "^5.0.2",
    "autoprefixer": "^10.4.16",
    "prettier": "^3.1.0",
    "query-string": "^8.1.0",
    "sass": "^1.53.0",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.0",
    "svelte-routing": "^2.6.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bulma": "^0.9.2",
    "sirv-cli": "^2.0.0"
  }
}
