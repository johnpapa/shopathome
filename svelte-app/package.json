{
  "name": "svelte-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "startx": "sirv public --cors --single --no-clear --port 5001",
    "validate": "svelte-check",
    "dev": "vite --port 5001",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "local-public": "npx @azure/static-web-apps-cli@latest start ./public --api-location http://localhost:7071",
    "local-x": "npx @azure/static-web-apps-cli@latest start ./public --api-location ../api",
    "local": "npx @azure/static-web-apps-cli@latest start http://localhost:5001 --api-location ../api "
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "prettier": "^2.2.1",
    "query-string": "^7.0.0",
    "sass": "^1.53.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-routing": "^1.5.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bulma": "^0.9.2",
    "sirv-cli": "^2.0.0"
  }
}
